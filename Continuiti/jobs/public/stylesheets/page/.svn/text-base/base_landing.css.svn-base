#landing_center {
  width: 500px;
  position: absolute;
  left: 20px;
  top: 20px;
  color: #fff;
  padding: 10px;
  font-size: 1.1em;
  line-height: 1.6em;
  background-color: rgba(0,0,0,0.8);
  border-radius: 15px;
  -moz-border-radius: 15px;
}

#landing_center hr {
  height: 1px;
  border: 0px;
  background-color: #fff;
}

#landing_center input {
  border: 1px solid #ccc;
  padding: 5px;
  margin: 3px;
  font-size: 123%;
  font-weight: bold;
}

#landing_center {
-moz-box-shadow: 0 0 5px 3px rgba(0,0,0,0.3);
-webkit-box-shadow: 0 0 5px 3px rgba(0,0,0,0.3);
box-shadow: 0 0 5px 3px rgba(0,0,0,0.3);
}

#landing_center a {
  color: #fff;
  font-weight: bold;
}

#landing_center h1 {
  font-size: 1.6em;
  line-height: 2.0em;
}

#landing_center #login_normal {
  float: left; 
  width: 59%;
}
#landing_center #login_connectors {
  float: left;
  width: 30%;
}
#LandingFooter{
    color: #fff;
    position: fixed;
    bottom: 0;
    left: 27px;
    z-index: 2;
    width: 3000px;
}
    
    #FooterContainer{
        font-family: Trebuchet MS;
        background:url(/images/footer/middle.png) repeat-x 0 0;
        height: 56px;
	width: 43%;
        float: left;
    }
    
    #FooterContainerLeft{
        background:url(/images/footer/left.png) no-repeat 0 0;
        height: 56px;
        width: 7px;
        float: left;
    }
    
    #FooterContainerRight{
        background: url(/images/footer/right.png) no-repeat 0 0;
        height: 56px;
        float: left;
        width: 7px;
    }
    
    #FooterNavigation{
        text-transform: uppercase;
        list-style: none;
        padding: 10px 20px;
        font-size: 12px;
        width: 420px;
        float: left;
        margin-top: 7px;
    }
    
        #FooterNavigation a {
            color: #fff;
        }
        
        #FooterNavigation a:hover{
            color: #7F7E7C;
        }
    
    #FooterNavigation li{
        display: inline;
    }
    
        #FooterNavigation li a{
            background-image: url(/images/footer/sprite.png);
            text-indent: -9999px;
            float: left;
            height: 22px;
        }
    
            a.ourStory{
                width: 100px;
                background-position: -1px 0;
                margin: 0 14px 0 0;
            }
            a.ourStory:hover{
                background-position: -1px -29px;
            }
        
            a.theBenefits{
                width: 113px;
                background-position: -103px 0;
                margin: 0 12px 0 0;
            }
            a.theBenefits:hover{
                background-position: -103px -29px;
            }
        
            a.destinations{
                width: 106px;
                background-position: -216px 0;
            }
            a.destinations:hover{
                background-position: -216px -29px;
            }
            
            a.press{
                width: 56px;
                background-position: -312px 0;
            }
            a.press:hover{
                background-position: -312px -29px;
            }
    
    #FooterCompanyInfo{
        color: #fff;
        font-size: 11px;
        margin: 15px 10px 0 0;
        line-height: 13px;
        float: left;
    }
    
        #FooterCompanyInfo a{
            color: #D5EBF8;
            text-transform: uppercase;
            clear: both;
        }


/*POP UPS*/
/*--Global CSS--*/
a{
	color:#1630d3;
	outline:none;
	text-decoration:none;
}
a img{border:none}
#wrapper{
	width:960px;
	margin-top:200px;
}
nav,header,section,fieldset,article,footer{display:block}
#logo{
    background: url(/images/popup/header.png) no-repeat;
	float:right;
    height: 167px;	
    width: 375px;
	margin:0;
}
#logo a{
	height:100%;
	width:100%;
	display:block;
	text-indent:-9999px;
}
.hide{display:none}

/*--Slider--*/
#slider header{margin-top:50px;}
.description{
	margin:60px 0;
}
.form{
	border-radius:10px;
	-moz-border-radius:10px;
	-webkit-border-radius:10px;	
	box-shadow:3px 3px 5px #000;
	-moz-box-shadow:3px 3px 5px #000;
	-webkit-box-shadow:3px 3px 5px #000;	
	background:#fff;
	position:relative;
	top:20px;
	left:10%;
	width:500px;
	margin:0px;
	padding:10px;
	border: 5px solid #A0A0A0;
}
.form_container{padding:10px}
.step{float:left}
fieldset{
	border:none;
	margin:0;
	padding:0 0 20px 20px;
}
fieldset h2{
	font:30px/700 Georgia,Serif;
	color:black;
	line-height:1;
	margin:10px 0 20px;
}
.button{
	background:#e1cde2;
	border-radius:10px;
	-moz-border-radius:10px;
	-webkit-border-radius:10px;
	height:60px;
	line-height:60px;
	position:relative;
	padding:5px;
}
.button_1{
	background:url("/images/popup/signup/continue.png") no-repeat;
	float:right;
	height:57px;
	width:172px;
	margin-top:3px;
	text-indent:-9999px;
}
.button_2{
	background:url("/images/popup/signup/finish.png") no-repeat;
	border:none;
	float:right;
	height:56px;
	width:261px;
	margin-top:3px;
	text-indent:-9999px;
}
.formhide{left:119%;opacity:.3;}
.form_2{top:-216px}
.form_2 p{
	font:12px georgia,serif;
	color:#adadad;
}

