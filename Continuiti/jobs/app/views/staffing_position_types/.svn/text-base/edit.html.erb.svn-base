<h1>Editing staffing_position_type</h1>

<% form_for(@staffing_position_type) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :active %><br />
    <%= f.check_box :active %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @staffing_position_type %> |
<%= link_to 'Back', staffing_position_types_path %>