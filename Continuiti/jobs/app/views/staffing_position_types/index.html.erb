<h1>Listing staffing_position_types</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Active</th>
  </tr>

<% @staffing_position_types.each do |staffing_position_type| %>
  <tr>
    <td><%=h staffing_position_type.title %></td>
    <td><%=h staffing_position_type.active %></td>
    <td><%= link_to 'Show', staffing_position_type %></td>
    <td><%= link_to 'Edit', edit_staffing_position_type_path(staffing_position_type) %></td>
    <td><%= link_to 'Destroy', staffing_position_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New staffing_position_type', new_staffing_position_type_path %>