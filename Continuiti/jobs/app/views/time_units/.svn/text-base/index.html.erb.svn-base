<h1>Listing time_units</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Active</th>
  </tr>

<% @time_units.each do |time_unit| %>
  <tr>
    <td><%=h time_unit.name %></td>
    <td><%=h time_unit.active %></td>
    <td><%= link_to 'Show', time_unit %></td>
    <td><%= link_to 'Edit', edit_time_unit_path(time_unit) %></td>
    <td><%= link_to 'Destroy', time_unit, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New time_unit', new_time_unit_path %>